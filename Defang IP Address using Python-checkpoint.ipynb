    "def ip_address(address):\n",
    "    new_address = \"\"\n",
    "    split_address = address.split(\".\")\n",
    "    seperator = \"[.]\"\n",
    "    new_address = seperator .join(split_address)\n",
    "    return new_address\n",
    "IP = input(\"Enter your IP address:\")\n",
    "ipaddress = ip_address(IP)\n",
    "print(ipaddress)\n",
    "\n",
